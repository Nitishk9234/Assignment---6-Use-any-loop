#include <stdio.h>
#include <conio.h>
int main()
{
     //I use long long because long long take 18
     long long count=0,n;
     printf("Enter a N value :");
     scanf("%lld",&n);
     printf("%lld",n);
    while(n!=0)
    {
        n=n/10;
        count++;
    }
    printf("Number of digit is %d",count);

    getch();
}
