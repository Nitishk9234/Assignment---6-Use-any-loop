#include <stdio.h>
#include <conio.h>

int main()
{
    int n,i,Flag=0;
    printf("enter a n value :");
    scanf("%d",&n);
    for(i=2;i<=n/2;i++)
    {
        if (n%i==0)
        {
           Flag=1;
        break;
        }
    }
    if (Flag==1)
        printf("not a prime ");
    else
        printf("prime");
    getch();

}
